 dy                     = 12000,
 e_we                   = 240, 201, 
 e_sn                   = 190, 233, 
 map_proj 		= 'lambert',
 ref_lat 		= 47.3,
 ref_lon 		= 5.2,
 truelat1 		= 30.0,
 truelat2 		= 60.0,
 stand_lon 		= 18.0,
