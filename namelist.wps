&share
 wrf_core = 'ARW',
 max_dom = 2,
 io_form_geogrid = 2,
 start_date = 2019-06-01_00:00:00,
 end_date   = 2019-06-03_00:00:00,
 interval_seconds = 21600,
 !nocolons = .true.,
/

&geogrid
