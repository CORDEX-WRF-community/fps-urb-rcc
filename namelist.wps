 geog_data_path         = '/share/geog/',
 opt_geogrid_tbl_path = './'
/

&ungrib
 out_format = 'WPS',
 prefix = 'FILE',
/

&metgrid
 constants_name = 'TAVGSFC',
 fg_name = 'FILE',
 io_form_metgrid = 2,
/


